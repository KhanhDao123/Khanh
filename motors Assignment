void setup() {
  // put your setup code here, to run once:
pinMode(8, INPUT);
pinMode(11,OUTPUT);
pinMode(12,OUTPUT);
}

void loop() {
  // put your main code here, to run repeatedly:
  if (digitalRead(8)==LOW){
  digitalWrite(11,HIGH);
  digitalWrite(12,LOW);
  }else{
  digitalWrite(11,LOW);
  digitalWrite(12,HIGH);
  }
}
